{% extends 'base.html.twig' %}

{% block title %}Company{% endblock %}

{% block body %}
    <h1>Company</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ company.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ company.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ company.description }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ company.telephone }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ company.adresse }}</td>
            </tr>
            <tr>
                <th>Code_postal</th>
                <td>{{ company.codePostal }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ company.logo }}</td>
            </tr>
            <tr>
                <th>Rcs</th>
                <td>{{ company.rcs }}</td>
            </tr>
            <tr>
                <th>Siret</th>
                <td>{{ company.siret }}</td>
            </tr>
            <tr>
                <th>FormLegal</th>
                <td>{{ company.formLegal }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ company.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>ValeurNominal</th>
                <td>{{ company.valeurNominal }}</td>
            </tr>
            <tr>
                <th>CodeValeur</th>
                <td>{{ company.codeValeur }}</td>
            </tr>
            <tr>
                <th>Siteweb</th>
                <td>{{ company.siteweb }}</td>
            </tr>
            <tr>
                <th>MotDuPresident</th>
                <td>{{ company.motDuPresident }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('company_index') }}">back to list</a>

    <a href="{{ path('company_edit', {'id': company.id}) }}">edit</a>

    {{ include('company/_delete_form.html.twig') }}
{% endblock %}
