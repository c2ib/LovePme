{% extends 'base.html.twig' %}

{% block body %}
<div class="bg-white p-5 contact-form">
{{ form_start(form) }}
<div class="form-group">
    {{ form_row(form.titre) }}
</div>
<div class="form-group">
    {{ form_row(form.message) }}
</div>
<div class="form-group">
    <button type="submit" class="btn btn-primary">send</button>
</div>
{{ form_end(form) }}
</div>
</div>
{% endblock %}
